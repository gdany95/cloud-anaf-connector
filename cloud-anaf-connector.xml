<service>
    <id>cloud-anaf-connector</id>
    <name>Cloud Anaf Connector</name>
    <description>Connects Linic billing with ANAF eFactura billing</description>
    <env name="CLOUD_ANAF_CONNECTOR_HOME" value="%BASE%"/>
    <executable>java</executable>
    <arguments>-Xmx256m -jar "%BASE%\cloud-anaf-connector.jar"</arguments>
    <logmode>rotate</logmode>
</service>